[ignore]
.*/koa-react/flow-typed/.*

[include]

[libs]
./flow-typed
../../flow-typed

[lints]

[options]
module.file_ext=.js
module.file_ext=.less
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=./src
esproposal.optional_chaining=enable

[strict]

[untyped]
.*/.*/node_modules/graphql/.*
.*/.*/node_modules/express-graphql/.*
.*/.*/node_modules/react-relay-network-modern/.*
.*/.*/node_modules/react-relay-network-modern-ssr/.*
