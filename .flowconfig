[ignore]
# ignore for @mikojs/eslint-config-base
<PROJECT_ROOT>/packages/eslint-config-base/src/__tests__/__ignore__
# ignore for @mikojs/server before flow support pipeline
<PROJECT_ROOT>/packages/create-project/src/__tests__/__ignore__/.*/src/server.js

[include]

[libs]
./flow-typed

[lints]

[options]
module.file_ext=.js
module.file_ext=.less
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=./src
module.system.node.resolve_dirname=./packages/create-project/src/__tests__/__ignore__/relay-server/src
esproposal.optional_chaining=enable
esproposal.decorators=ignore
experimental.const_params=true

[strict]
