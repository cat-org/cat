[ignore]
# ignore for @cat-org/eslint-config-cat
<PROJECT_ROOT>/packages/eslint-config-cat/src/__tests__/__ignore__

[include]

[libs]
./flow-typed
./packages/**/lib/flow-typed

[lints]

[options]
module.ignore_non_literal_requires=true
esproposal.optional_chaining=enable

# name mapper for @cat-org/configs
module.name_mapper='^@cat-org\/configs\/lib\/\(.*\)$' -> '<PROJECT_ROOT>/packages/configs/src/\1'

[strict]
