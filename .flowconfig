[ignore]

[include]

[libs]
./flow-typed
../../flow-typed

[lints]

[options]
module.file_ext=.js
module.file_ext=.less
module.system.node.allow_root_relative=true
module.system.node.root_relative_dirname=./src
esproposal.optional_chaining=enable

[strict]

[untyped]
# FIXME: flow-mono-cli error https://github.com/ImmoweltGroup/flow-mono-cli/issues/310
.*/node_modules/graphql/.*
.*/node_modules/express-graphql/.*
