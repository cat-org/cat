[ignore]
# ignore for @cat-org/eslint-config-cat
<PROJECT_ROOT>/packages/eslint-config-cat/src/__tests__/__ignore__

[include]

[libs]
./flow-typed

[lints]

[options]
module.ignore_non_literal_requires=true
esproposal.optional_chaining=enable

# name mapper for @cat-org/<packages>
module.name_mapper='^@cat-org\/\(.*\)\/lib\/\(.*\)$' -> '<PROJECT_ROOT>/packages/\1/src/\2'
module.name_mapper='^@cat-org\/\(.*\)$' -> '<PROJECT_ROOT>/packages/\1'

[strict]
