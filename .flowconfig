[ignore]
# ignore for @mikojs/eslint-config-base
<PROJECT_ROOT>/packages/eslint-config-base/src/__tests__/__ignore__

[include]

[libs]
./flow-typed

[lints]

[options]
module.file_ext=.js
module.file_ext=.less
module.system.node.allow_root_relative=true
module.system.node.root_relative_dirname=./packages/create-project/src/__tests__/__ignore__/relay-server/src
module.system.node.root_relative_dirname=./packages/create-project/src/__tests__/__ignore__/relay-server-with-css/src
module.system.node.root_relative_dirname=./packages/create-project/src/__tests__/__ignore__/relay-server-with-less/src
module.system.node.root_relative_dirname=./packages/create-project/src/__tests__/__ignore__/lerna/relay-server/src
module.system.node.root_relative_dirname=./packages/create-project/src/__tests__/__ignore__/lerna/relay-server-with-css/src
module.system.node.root_relative_dirname=./packages/create-project/src/__tests__/__ignore__/lerna/relay-server-with-less/src
esproposal.optional_chaining=enable
esproposal.decorators=ignore
experimental.const_params=true

[strict]
