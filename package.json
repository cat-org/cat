{
  "private": true,
  "name": "@cat-org/root",
  "license": "MIT",
  "scripts": {
    "clean:all": "make clean-all",
    "precommit": "make babel-lint-staged && configs-scripts lint-staged",
    "preinstall": "rm -rf ./node_modules/@cat-org",
    "install:all": "make install-all",
    "lint": "NODE_ENV=test configs-scripts esw --cache --ext .js",
    "lint:watch": "yarn lint -w --rule \"prettier/prettier: off\" --quiet",
    "prepare":
      "patch-package && ln -snf $(pwd)/packages ./node_modules/@cat-org",
    "prod": "NODE_ENV=production make babel-all",
    "release": "make release",
    "test": "make babel-test && NODE_ENV=test configs-scripts jest --silent"
  },
  "dependencies": {
    "babel-eslint": "^8.2.3",
    "eslint-config-fbjs": "^2.0.1",
    "eslint-config-google": "^0.9.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-flowtype": "^2.46.2",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsdoc": "^3.6.3",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-relay": "^0.0.24"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.54",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-flow": "^7.0.0-beta.54",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-imports": "^1.5.0",
    "eslint": "^5.1.0",
    "eslint-watch": "^4.0.2",
    "flow-bin": "^0.76.0",
    "flow-typed": "^2.5.1",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "lerna": "^2.11.0",
    "lerna-changelog": "^0.8.0",
    "lint-staged": "^7.2.0",
    "patch-package": "^5.1.1",
    "postinstall-prepare": "^1.0.1",
    "prettier": "^1.13.7",
    "prettier-package-json": "^1.6.0"
  },
  "engines": {
    "node": ">= 10.2.1",
    "npm": ">= 6.1.0",
    "yarn": ">= 1.7.0"
  }
}
